                                                                    <table class="table">
                                                                        <tr>
                                                                            <th>Pofile Picture</th>
                                                                            <th>Trainer Name</th>
                                                                            <th>Joining Date</th>
                                                                            <th>Contact No</th>
                                                                            <th>Total Members</th>
                                                                            <th>Total Reports</th>
                                                                            
                                                                            <!-- <th>Transaction ID</th> -->
                                                                        </tr>
                                                                        <% if(trainers.length > 0) { %>
                                                                            <% trainers.forEach((trainer,index) => { %>
                                                                        <tr>
                                                                            <td><img src="/assets/images/product/prod3.jpg" alt="prod img" class="img-fluid"></td>
                                                                            <td><%=trainer.name%></td>
                                                                            <td><%=trainer.registration_date%></td>
                                                                            <td><%=trainer.contact_no%></td>
                                                                             <td><%=trainer.members_cnt%></td>
                                                                            <td>0</td>
                                                                        </tr>
                                                                           <% }); %>
                                                                        <% } %>
                                                                      
                                                                    </table>
                                                                </div>
                                                                <div class="text-center">
                                                                    <button class="btn btn-outline-primary btn-round btn-sm">Load More</button>
                                                                </div>
                                                            </div>
                                                            <div class="tab-pane" id="messages3" role="tabpanel">

                                                                <div class="table-responsive">
                                                                    <table class="table">
                                                                        <tr>
                                                                            <th>Equipment</th>
                                                                            <th>Equipment Name</th>
                                                                            <th>Equipment Quantity</th>
                                                                            <!-- <th>Equipment Reports</th> -->
                                                                            <th>Equipment Reports</th>
                                                                            <th>Fix Equipment</th>
                                                                            <!-- <th>Transaction ID</th>  -->
                                                                        </tr>
                                                                        <% for(var i = 0;i < equipments.length;i++) { %>
                                                                        <tr>
                                                                            <td><img src="/assets/images/product/prod1.jpg" alt="prod img" class="img-fluid"></td>
                                                                            <td><%=equipments[i].name%></td>
                                                                            <td><%=equipments[i].total_cnt%></td>
                                                                            <td><span class="label label-primary"><%=equipments[i].reports%></span></td>
                                                                            <% if(equipments[i].reports > 2){ %>
                                                                            <td>
                                                                            <form action="/admin/dashboard/FixEquipment" method="POST">
                                                                                 <input type="hidden" name="equipment_id" value="<%=equipments[i]._id%>">
                                                                                 <button type="submit" class="danger">Fix Equipment</button>
                                                                            </form>
                                                                            </td>
                                                                            <% } else { %>
                                                                            <td><div style="cursor: pointer;" class="success">Equipment Working</div></td>
                                                                            <% } %>

                                                                        </tr>
                                                                        <% } %>
                                                                       
                                                                    </table>
                                                                </div>
                                                                <div class="text-center">
                                                                    <button class="btn btn-outline-primary btn-round btn-sm">Load More</button>
                                                                </div>
                                                            </div>
                                                            <div class="tab-pane" id="settings3" role="tabpanel">

                                                                <div class="table-responsive">
                                                                    <table class="table">
                                                                        <tr>
                                                                            <th>#</th>
                                                                            <th>Reported By</th>
                                                                            <th>Reported For</th>
                                                                            <th>Report Date</th>
                                                                            <th>Report Type</th>
                                                                           
                                                                        </tr>
                                                                        <% for(var i = 0;i < reports.length;i++){ %>
                                                                        <tr>
                                                                            <td><%=(i+1)%></td>
                                                                            <td><%=reports[i].reported_by%></td>
                                                                            <td><%=reports[i].reported_for%></td>
                                                                            <td><%=reports[i].report_date%></td>
                                                                            <td><%=reports[i].report_type%></td>
                                                                        </tr>
                                                                        <% } %> 
                                                                    </table>
                                                                </div>
                                                                <div class="text-center">
                                                                    <button class="btn btn-outline-primary btn-round btn-sm">Load More</button>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <!-- tabs card end -->

                                        </div>
                                    </div>

